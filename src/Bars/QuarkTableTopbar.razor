@inherits ComponentBase

<div class="quark-table-topbar-wrapper">
    <div class="quark-table-topbar @LayoutClass">
        @ChildContent
    </div>
</div>

@code {
    [Parameter]
    public RenderFragment? ChildContent { get; set; }

    [Parameter]
    public string LayoutClass { get; set; } = string.Empty;
}